# Vehicle Dimension Detection Configuration

# Enable/disable vehicle dimension detection
DIMENSION_ENABLED=false

# Path to ML detection model (leave empty for default mock detection)
DIMENSION_MODEL_PATH=

# Detection confidence threshold (0.0 - 1.0)
DIMENSION_THRESHOLD=0.5

# Camera Calibration Parameters
# These values MUST be calibrated for your specific camera setup

# Focal length in pixels (typically 800-1500 for standard cameras)
CAMERA_FOCAL_LENGTH=1000.0

# Image resolution
CAMERA_IMAGE_WIDTH=1920
CAMERA_IMAGE_HEIGHT=1080

# Camera mounting height from ground in meters
CAMERA_HEIGHT_METERS=6.0

# Camera tilt angle in degrees (0 = horizontal, 90 = pointing straight down)
CAMERA_TILT_ANGLE=30.0

# Reference object calibration
# Measure a known object in the scene (e.g., road marking, pole)
# Count the pixels it occupies in the image
CAMERA_REF_PIXEL_LENGTH=200

# Real-world length of the reference object in meters
CAMERA_REF_REAL_LENGTH=5.0

# Distance from camera to the reference object in meters
CAMERA_REF_DISTANCE=10.0
